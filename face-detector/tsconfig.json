{
  "compilerOptions": {
    "lib": ["webworker", "es6", "scripthost"],
    "outDir": "./build",
    "allowJs": true,
    "jsx": "react",
    "declaration": true,
    "target": "es5",
    "module": "es6",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./src/*"
  ]
}
